<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs author="RooX Solutions" author_email="support@roox.ru" height="100%" title="IODemo" version="1.0-SNAPSHOT" width="100%">
                 version="2.1">
    </ModulePrefs>
    <Content content-type="text/html" type="html">
        <![CDATA[
        <script type="text/javascript" src="IODemo/js/UIController.js"></script>
        <script language="javascript" type="text/javascript">
            function init() {
                try {
                    roox.UIController = new roox.UIController(__MODULE_ID__);
                } catch (e) {
                    gadgets.log(e)
                }
            }
            gadgets.util.registerOnLoadHandler(init);
        </script>

        <div id="widget__MODULE_ID__" style="width:100%; height: 100%;">
            <div style="margin: 30px;">
            <h3>
                <input type="button" value="Get WRS Version :" style="padding: 10px;"
                       onclick="roox.UIController.getVersionAction(); return false;"/>
                <span id="webapi-version"></span>
                <br>
                <input type="button" value="Test http://ya.ru/" style="padding: 10px;"
                                       onclick="roox.UIController.testAction(); return false;"/>
            </h3>

            </div>
        </div>
        ]]>
    </Content>
</Module>
